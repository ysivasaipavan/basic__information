{{#each schemas}}

{{#if (isEnum)}}
{{>validationEnum}}
{{else if (isObject)}}
{{>validationGeneric}}
{{else if (isOneOf)}}
{{>validationOneOf}}
{{else if (isAnyOf)}}
{{>validationOneOf}}
{{else if (isInterface)}}
{{>validationInterface}}
{{else}}
{{warn (concat 'Unsupported schema type: ' schemaType)}}
{{/if}}
{{/each}}
